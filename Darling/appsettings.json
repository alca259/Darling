{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console",
        "restrictedToMinimumLevel": "Information"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": 3,
          "retainedFileCountLimit": 10
        },
        "restrictedToMinimumLevel": "Information"
      }
    ]
  },
  "AppOptions": {
    "ProcessName": "MySingingMonsters",
    "Language": "es-ES",
    "Version": "1.2.0",
    "Name": "Darling",
    "ThresholdButtons": 0.75,
    "ThresholdMapIslandText": 0.60,
    "TempImageFileName": "AlcaTempFile.jpg",
    "DelayMilliseconds": [
      {
        "Key": "FindProcess",
        "Value": 300
      },
      {
        "Key": "MouseClick",
        "Value": 300
      },
      {
        "Key": "WindowToTop",
        "Value": 200
      },
      {
        "Key": "DiamondsWait",
        "Value": 2500
      },
      {
        "Key": "DefaultWaitBetweenActions",
        "Value": 300
      },
      {
        "Key": "IslandStay",
        "Value": 1000
      },
      {
        "Key": "IslandFindButton",
        "Value": 300
      },
      {
        "Key": "IslandPopupWait",
        "Value": 500
      },
      {
        "Key": "NextIsland",
        "Value": 800
      },
      {
        "Key": "EnterIsland",
        "Value": 800
      },
      {
        "Key": "AfterEnterIsland",
        "Value": 3000
      },
      {
        "Key": "NextVoteIsland",
        "Value": 5000
      }
    ]
  }
}